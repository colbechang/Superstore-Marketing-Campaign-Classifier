        "\n",
        "# Running the model with 1000 epochs and batch sizes of 512\n",
        "history = model.fit(\n",
        "    X_train, y_train,\n",
        "    validation_data=(X_train, y_train),\n",
        "    batch_size=512,\n",
        "    epochs=1000,\n",
        "    callbacks=[early_stopping],\n",
        "    verbose = 0\n",
